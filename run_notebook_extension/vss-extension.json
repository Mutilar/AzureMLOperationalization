{
    "manifestVersion": 1,
    "id": "build-release-task",
    "name": "Agentlessly Run Notebook",
    "version": "1.2.0",
    "publisher": "t-brhung",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Agentlessly Running Notebooks via an Azure Function",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "icon.png"        
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}